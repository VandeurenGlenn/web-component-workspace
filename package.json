{
  "name": "web-component-workspace",
  "version": "1.1.0",
  "description": "Quick bower-based workspace buidler.",
  "bin": {
    "wcw": "bin/web-component-workspace.js"
  },
  "scripts": {
    "jshint": "node_modules/.bin/jshint bin/ lib/",
    "test": "npm run jshint",
    "push": "git push && git push --tags && npm publish",
    "release-patch": "mversion patch -m && npm run push",
    "release-minor": "mversion minor -m && npm run push",
    "release-major": "mversion patch -m && npm run push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PolymerLabs/web-component-workspace.git"
  },
  "author": "The Polymer Project Authors",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/PolymerLabs/web-component-workspace/issues"
  },
  "homepage": "https://github.com/PolymerLabs/web-component-workspace",
  "dependencies": {
    "bower": "^1.4.1",
    "es6-promise": "^2.3.0",
    "nedb": "^1.1.2",
    "nodegit": "^0.23.0",
    "path": "^0.11.14",
    "shelljs": "^0.5.1",
    "temp": "^0.8.3",
    "yargs": "^3.11.0"
  },
  "devDependencies": {
    "jshint": "^2.8.0"
  }
}
